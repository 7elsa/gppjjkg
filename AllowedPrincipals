(["LoadbalancerName", "SecurityGroup"] | @csv),
(
  .LoadBalancers[] |
  .LoadbalancerName as $lbName |
  ( .SecurityGroups // [] | if length == 0 then [""] else . end )[] as $sg |
  {
    LoadbalancerName: $lbName,
    SecurityGroup: ($sg // "")
  } |
  [.LoadbalancerName, .SecurityGroup] | @csv
)
